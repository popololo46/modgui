<http://calf.sourceforge.net/team>
    a foaf:Person ;
    foaf:name "Calf Studio Gear" ;
    foaf:mbox <mailto:info@calf-studio-gear.org> ;
    foaf:homepage <http://calf-studio-gear.org/> .


:in a pg:StereoGroup , pg:InputGroup ;
    lv2:symbol "in" ;
    rdfs:label "Input" .

:out a pg:StereoGroup , pg:OutputGroup ;
    lv2:symbol "out" ;
    rdfs:label "Output" .

<http://calf.sourceforge.net/plugins/RotarySpeaker> a lv2:Plugin, lv2:SimulatorPlugin ;
    doap:name "Calf Rotary Speaker" ;
    doap:license <http://usefulinc.com/doap/licenses/lgpl> ;
    doap:developer <http://calf.sourceforge.net/team> ;
    doap:maintainer <http://calf.sourceforge.net/team> ;
    dct:replaces <urn:ladspa:33923> ;
    lv2:optionalFeature epp:supportsStrictBounds ;
    lv2:optionalFeature lv2:hardRTCapable ;
    lv2:optionalFeature <http://lv2plug.in/ns/ext/urid#map> ;
    pg:mainInput :in ;
    pg:mainOutput :out ;
    lv2:port [
        a lv2:InputPort ;
        a lv2:AudioPort ;
        lv2:index 0 ;
        lv2:symbol "in_l" ;
        lv2:name "In L" ;
        lv2:designation pg:left ;
        pg:group :in ;
    ] , [
        a lv2:InputPort ;
        a lv2:AudioPort ;
        lv2:index 1 ;
        lv2:symbol "in_r" ;
        lv2:name "In R" ;
        lv2:portProperty lv2:connectionOptional ;
        lv2:designation pg:right ;
        pg:group :in ;
    ] , [
        a lv2:OutputPort ;
        a lv2:AudioPort ;
        lv2:index 2 ;
        lv2:symbol "out_l" ;
        lv2:name "Out L" ;
        lv2:designation pg:left ;
        pg:group :out ;
    ] , [
        a lv2:OutputPort ;
        a lv2:AudioPort ;
        lv2:index 3 ;
        lv2:symbol "out_r" ;
        lv2:name "Out R" ;
        lv2:designation pg:right ;
        pg:group :out ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 4 ;
        lv2:symbol "vib_speed" ;
        lv2:name "Speed Mode" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:integer ;
        lv2:portProperty lv2:enumeration ;
        lv2:scalePoint [ rdfs:label "Off"; rdf:value 0 ] ;
        lv2:scalePoint [ rdfs:label "Chorale"; rdf:value 1 ] ;
        lv2:scalePoint [ rdfs:label "Tremolo"; rdf:value 2 ] ;
        lv2:scalePoint [ rdfs:label "HoldPedal"; rdf:value 3 ] ;
        lv2:scalePoint [ rdfs:label "ModWheel"; rdf:value 4 ] ;
        lv2:scalePoint [ rdfs:label "Manual"; rdf:value 5 ] ;
        lv2:default 5 ;
        lv2:minimum 0 ;
        lv2:maximum 5 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 5 ;
        lv2:symbol "spacing" ;
        lv2:name "Tap Spacing" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:default 0.500000 ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 6 ;
        lv2:symbol "shift" ;
        lv2:name "Tap Offset" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:default 0.500000 ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 7 ;
        lv2:symbol "mod_depth" ;
        lv2:name "FM Depth" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:default 0.600000 ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 8 ;
        lv2:symbol "treble_speed" ;
        lv2:name "Treble Motor" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty epp:logarithmic ;
        lv2:default 36 ;
        lv2:minimum 10 ;
        lv2:maximum 600 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 9 ;
        lv2:symbol "bass_speed" ;
        lv2:name "Bass Motor" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty epp:logarithmic ;
        lv2:default 30 ;
        lv2:minimum 10 ;
        lv2:maximum 600 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 10 ;
        lv2:symbol "mic_distance" ;
        lv2:name "Mic Distance" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:default 0.700000 ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 11 ;
        lv2:symbol "reflection" ;
        lv2:name "Reflection" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:default 0.300000 ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 12 ;
        lv2:symbol "am_depth" ;
        lv2:name "AM Depth" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:default 0.600000 ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 13 ;
        lv2:symbol "test" ;
        lv2:name "Test" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
    ] , [
        a lv2:OutputPort ;
        a lv2:ControlPort ;
        lv2:index 14 ;
        lv2:symbol "meter_l" ;
        lv2:name "Low rotor" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:connectionOptional ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
    ] , [
        a lv2:OutputPort ;
        a lv2:ControlPort ;
        lv2:index 15 ;
        lv2:symbol "meter_h" ;
        lv2:name "High rotor" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:connectionOptional ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 16 ;
        lv2:symbol "bypass" ;
        lv2:name "Bypass" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:toggled ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 17 ;
        lv2:symbol "level_in" ;
        lv2:name "Input Gain" ;
        lv2:default 1 ;
        lv2:minimum 0.0156250 ;
        lv2:maximum 64 ;
    ] , [
        a lv2:InputPort ;
        a lv2:ControlPort ;
        lv2:index 18 ;
        lv2:symbol "level_out" ;
        lv2:name "Output Gain" ;
        lv2:default 2 ;
        lv2:minimum 0.0156250 ;
        lv2:maximum 64 ;
    ] , [
        a lv2:OutputPort ;
        a lv2:ControlPort ;
        lv2:index 19 ;
        lv2:symbol "meter_inL" ;
        lv2:name "Meter-InL" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:connectionOptional ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
    ] , [
        a lv2:OutputPort ;
        a lv2:ControlPort ;
        lv2:index 20 ;
        lv2:symbol "meter_inR" ;
        lv2:name "Meter-InR" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:connectionOptional ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
    ] , [
        a lv2:OutputPort ;
        a lv2:ControlPort ;
        lv2:index 21 ;
        lv2:symbol "meter_outL" ;
        lv2:name "Meter-OutL" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:connectionOptional ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
    ] , [
        a lv2:OutputPort ;
        a lv2:ControlPort ;
        lv2:index 22 ;
        lv2:symbol "meter_outR" ;
        lv2:name "Meter-OutR" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:connectionOptional ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
    ] , [
        a lv2:OutputPort ;
        a lv2:ControlPort ;
        lv2:index 23 ;
        lv2:symbol "clip_inL" ;
        lv2:name "0dB-InL" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:connectionOptional ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
    ] , [
        a lv2:OutputPort ;
        a lv2:ControlPort ;
        lv2:index 24 ;
        lv2:symbol "clip_inR" ;
        lv2:name "0dB-InR" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:connectionOptional ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
    ] , [
        a lv2:OutputPort ;
        a lv2:ControlPort ;
        lv2:index 25 ;
        lv2:symbol "clip_outL" ;
        lv2:name "0dB-OutL" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:connectionOptional ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
    ] , [
        a lv2:OutputPort ;
        a lv2:ControlPort ;
        lv2:index 26 ;
        lv2:symbol "clip_outR" ;
        lv2:name "0dB-OutR" ;
        lv2:portProperty epp:hasStrictBounds ;
        lv2:portProperty lv2:connectionOptional ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
    ] , [
        a lv2:InputPort ;
        a atom:AtomPort ;
        lv2:index 27 ;
        lv2:symbol "midi_in" ;
        lv2:name "MIDI In" ;
        lv2:portProperty lv2:connectionOptional ;
        atom:bufferType atom:Sequence ;
        atom:supports lv2midi:MidiEvent ;
        atom:supports atom:Property ;

    ] , [
        a lv2:OutputPort ;
        a atom:AtomPort ;
        lv2:index 28 ;
        lv2:symbol "events_out" ;
        lv2:name "Events" ;
        lv2:portProperty lv2:connectionOptional ;
        atom:bufferType atom:Sequence ;
        atom:supports lv2midi:MidiEvent ;
        atom:supports atom:Property ;

    ]
.


