@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://zynaddsubfx.sourceforge.net/fx#Chorus>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-zynchorus.html> ;
        modgui:stylesheet <modgui/stylesheet-zynchorus.css> ;
        modgui:screenshot <modgui/screenshot-zynchorus.png> ;
        modgui:thumbnail <modgui/thumbnail-zynchorus.png> ;
        modgui:brand "ZynFusion" ;
        modgui:label "ZynChorus" ;
        modgui:model "boxy" ;
        modgui:panel "8-sliders" ;
        modgui:color "purple" ;
        modgui:knob "purple" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "lfofreq" ;
            lv2:name "LFO Freq" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "lforand" ;
            lv2:name "LFO Random" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "lfotype" ;
            lv2:name "LFO Type" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "lfostereo" ;
            lv2:name "LFO Stereo" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "depth" ;
            lv2:name "Depth" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "delay" ;
            lv2:name "Delay" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "fb" ;
            lv2:name "Feed back" ;
        ] , [
            lv2:index 7 ;
            lv2:symbol "lrcross" ;
            lv2:name "L/R Cross" ;
        ] ;
    ] .