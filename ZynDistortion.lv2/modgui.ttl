@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://zynaddsubfx.sourceforge.net/fx#Distortion>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-zyndistortion.html> ;
        modgui:stylesheet <modgui/stylesheet-zyndistortion.css> ;
        modgui:screenshot <modgui/screenshot-zyndistortion.png> ;
        modgui:thumbnail <modgui/thumbnail-zyndistortion.png> ;
        modgui:brand "ZynFusion" ;
        modgui:label "ZynDistortion" ;
        modgui:model "boxy" ;
        modgui:panel "7-sliders" ;
        modgui:color "purple" ;
        modgui:knob "purple" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "lrcross" ;
            lv2:name "L/R Cross" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "drive" ;
            lv2:name "Drive" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "level" ;
            lv2:name "Level" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "type" ;
            lv2:name "Type" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "negate" ;
            lv2:name "Negate" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "lpf" ;
            lv2:name "Low-Pass" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "hpf" ;
            lv2:name "High-Pass" ;
        ] ;
    ] .