	lv2:default 0;
],

######################################################################
# FILTER                                                                                                                                               #
#                                                                 							                                        #
######################################################################

[
	a lv2:ControlPort, lv2:InputPort;
	lv2:index 6;
	lv2:symbol "filter_mode";
