@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<https://github.com/lkjbdsp/lkjb-plugins#ReFine>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-refine.html> ;
        modgui:stylesheet <modgui/stylesheet-refine.css> ;
        modgui:screenshot <modgui/screenshot-refine.png> ;
        modgui:thumbnail <modgui/thumbnail-refine.png> ;
        modgui:brand "Lkjb" ;
        modgui:label "ReFine" ;
        modgui:model "boxy" ;
        modgui:panel "4-knobs" ;
        modgui:color "wood0" ;
        modgui:knob "purple" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "red" ;
            lv2:name "Warm" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "green" ;
            lv2:name "Space" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "blue" ;
            lv2:name "Punch" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "x2" ;
            lv2:name "x2" ;
        ] ;
    ] .